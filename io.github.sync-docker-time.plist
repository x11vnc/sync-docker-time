<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>io.github.sync-docker-time</string>
	<key>ProgramArguments</key>
	<array>
		<string>~/Library/SyncDockerTime/bin/sleepwatcher-sdt</string>
		<string>-V</string>
		<string>-w ~/Library/SyncDockerTime/bin/sync-docker-time</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>StandardOutPath</key>
	<string>~/.sync-docker-time.log</string>
	<key>StandardErrorPath</key>
	<string>~/.sync-docker-time.log</string>
</dict>
</plist>
